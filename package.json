{
  "name": "anybot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Any Association Bot",
  "author": "Any Association",
  "license": "MIT",
  "homepage": "https://github.com/anyproto",
  "keywords": [
    "probot",
    "github",
    "probot-app"
